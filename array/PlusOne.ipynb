   "source": [
    "def plusone (nums):\n",
    "    carry = 1\n",
    "    for i in range (1, len(nums) + 1):\n",
    "        digit = (nums[-i] + carry)\n",
    "        nums[-i] = digit % 10\n",
    "        carry = digit // 10\n",
    "    if carry == 1:\n",
    "        nums.insert(0, carry)\n",
    "\n",
    "    return nums"
   ],
